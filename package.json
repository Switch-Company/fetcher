{
  "name": "@switch-company/fetcher",
  "version": "1.0.0",
  "description": "Wrap the Fetch API with convenience methods.",
  "main": "dist/fetcher.js",
  "module": "lib/index.js",
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "build": "babel src -d lib && rollup -c && rollup -c rollup.config.minified.js",
    "test": "tape test/*.js | tap-diff",
    "version": "npm run test && npm run build && git add -A dist",
    "postversion": "git push && git push --tags",
    "watch:build": "watch 'npm run build' src --interval=1",
    "watch:test": "watch 'npm test' {dist,test} --interval=1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Switch-Company/fetcher.git"
  },
  "author": {
    "name": "Switch",
    "email": "bonjour@switch.paris",
    "url": "http://switch.paris"
  },
  "license": "ISC",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 10"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "busboy": "^0.2.14",
    "eslint": "^4.19.1",
    "eslint-config-idiomatic": "^4.0.0",
    "puppeteer": "^1.4.0",
    "rollup": "^0.57.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "tap-diff": "^0.1.1",
    "tape": "^4.9.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@switch-company/form-utils": "^1.0.0"
  }
}
